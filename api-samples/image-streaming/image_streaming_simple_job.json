{
  "taskGroups": [{
    "taskCount": "1",
    "taskCountPerNode": "1",
    "taskSpec": {
      "runnables": [{
        "container": {
          "imageUri": "us-central1-docker.pkg.dev/test-project/my-repository/torchx:latest",
          "commands": ["-c", "print(\"Hello World\")"],
          "entrypoint": "python",
          "volumes": ["/var/lib/google:/var/lib/google:rw"],
          "enableImageStreaming": true
        }
      }]
    }
  }],
  "allocationPolicy": {
    "instances": [{
      "policy": {
        "machineType": "e2-standard-4"
      }
    }]
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}